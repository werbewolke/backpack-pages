            "homepage": "https://werbewolke.de"
        }
    ],
    "homepage": "https://github.com/werbewolke/pages",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "Pages"
    ],
    "require": {
        "illuminate/support": "~8|~9",
        "backpack/filemanager": "^2.0",
        "backpack/pro": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Werbewolke\\Pages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Werbewolke\\Pages\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Werbewolke\\Pages\\AddonServiceProvider"
            ]
        }
    }
}
